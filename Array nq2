
#include <stdio.h>
#define example 10
int main(){
    int balance = 1000;
    int transactions[example] = {-200,-150,-500,-400,-50};
    int numbertransac = 5;
    for(int i = 0; i<numbertransac;i++){
        int transaction = transactions[i];

    
    if ( transaction <0 && -transaction < balance){
        printf ("Invalid transaction");
    }else {
        balance = transaction + balance;
        printf("Done");
    }
    }
return 0;
}
